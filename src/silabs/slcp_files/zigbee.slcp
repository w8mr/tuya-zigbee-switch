config_file:
- {path: ../zcl_config.zap, directory: zcl}
- override:
    component: iostream_usart
    file_id: iostream_usart_config
    instance: inst
  path: ../../silabs/config/sl_iostream_usart_inst_config.h
- path: ../../silabs/config/silabs_config.h
configuration:
- {name: SL_BOARD_ENABLE_VCOM, value: '1'}
- condition: [iostream_usart]
  name: SL_IOSTREAM_USART_VCOM_FLOW_CONTROL_TYPE
  value: usartHwFlowControlCtsAndRts
- condition: [iostream_eusart]
  name: SL_IOSTREAM_EUSART_VCOM_FLOW_CONTROL_TYPE
  value: eusartHwFlowControlCtsAndRts
- {name: SL_CLI_MAX_INPUT_ARGUMENTS, value: '16'}
- {name: SLI_ZIGBEE_PRIMARY_NETWORK_DEVICE_TYPE, value: SLI_ZIGBEE_NETWORK_DEVICE_TYPE_ROUTER}
- {name: SLI_ZIGBEE_PRIMARY_NETWORK_SECURITY_TYPE, value: SLI_ZIGBEE_NETWORK_SECURITY_TYPE_3_0}
- condition: [device_series_1]
  name: NVM3_DEFAULT_NVM_SIZE
  value: '36864'
- condition: [device_series_2]
  name: NVM3_DEFAULT_NVM_SIZE
  value: '32768'
- {name: SL_PSA_ITS_SUPPORT_V1_DRIVER, value: '0'}
- {name: SL_PSA_ITS_SUPPORT_V2_DRIVER, value: '0'}
- {name: SL_PSA_ITS_SUPPORT_V3_DRIVER, value: '1'}
- {name: SL_ZIGBEE_AF_PLUGIN_REPORTING_TABLE_SIZE, value: '32'}
- {name: SL_ZIGBEE_BINDING_TABLE_SIZE, value: '32'}
- {name: SL_ZIGBEE_AF_PLUGIN_OTA_STORAGE_SIMPLE_EEPROM_GECKO_BOOTLOADER_STORAGE_SUPPORT, value: 'USE_FIRST_SLOT'}  
- {name: SL_ZIGBEE_AF_PLUGIN_OTA_CLIENT_AUTO_START, value: '1'}
- {name: SL_ZIGBEE_AF_PLUGIN_OTA_CLIENT_POLICY_FIRMWARE_VERSION, value: 0}
- {name: SL_ZIGBEE_AF_PLUGIN_OTA_CLIENT_POLICY_IMAGE_TYPE_ID, value: 54179}
- {name: SL_IOSTREAM_USART_INST_PERIPHERAL, value: 'USART1'}
- {name: SL_IOSTREAM_USART_INST_PERIPHERAL_NO, value: '1'}
- {name: SL_IOSTREAM_USART_INST_TX_PORT, value: 'gpioPortB'}
- {name: SL_IOSTREAM_USART_INST_TX_PIN, value: '1'}
- {name: SL_IOSTREAM_USART_INST_RX_PORT, value: 'gpioPortB'}
- {name: SL_IOSTREAM_USART_INST_RX_PIN, value: '2'}
- {name: DEFAULT_CONFIG, value: 'silabs_generic;silabs_generic;'}
- {name: SL_ZIGBEE_PACKET_BUFFER_HEAP_SIZE, value: SL_ZIGBEE_MEDIUM_PACKET_BUFFER_HEAP}
ui_hints: {}
define: 
  - name: HAL_SILABS
    value: '1'

